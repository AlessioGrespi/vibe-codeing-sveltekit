# Database
DATABASE_URL="postgresql://username:password@localhost:5432/database_name"

# OAuth Providers
GOOGLE_CLIENT_ID="your-google-client-id"
GOOGLE_CLIENT_SECRET="your-google-client-secret"
GITHUB_CLIENT_ID="your-github-client-id"
GITHUB_CLIENT_SECRET="your-github-client-secret"

# OAuth Redirect URIs
# Development (localhost)
OAUTH_REDIRECT_URI_GOOGLE="http://localhost:5173/oauth/callback/google"
OAUTH_REDIRECT_URI_GITHUB="http://localhost:5173/oauth/callback/github"

# Production (replace with your actual domain)
# OAUTH_REDIRECT_URI_GOOGLE="https://your-domain.com/oauth/callback/google"
# OAUTH_REDIRECT_URI_GITHUB="https://your-domain.com/oauth/callback/github"

# To switch between environments, simply uncomment the production lines
# and comment out the development lines in your .env file
